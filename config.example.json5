  "apiUrl": "https://jsonplaceholder.typicode.com",
  "cache":  [ "/posts", "/post/:slug" ], // Only GET requests allowed for now
  "bust": {
    "POST": {
      "/posts": [ "/posts", "/posts/count" ]
    },
    "PUT": {
      "/post/:slug": [ "/posts", "/post/:slug" ]
    },
    "DELETE": {
      "/post/:slug": [ "/posts", "/post/:slug" ]
    },
  }
}